{
  "expo": {
    "name": "Travel Planner AI",
    "slug": "travel-planner-ai",
    "owner": "jurrianl",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/bouquet.png",
    "scheme": "travelplannerai",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/bouquet.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "appleTeamId": "",
      "bundleIdentifier": "app.jurrian.travelplannerai",
      "icon": {
        "light": "./assets/icons/bouquet.png",
        "dark": "./assets/icons/floral.png"
      },
      "supportsTablet": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.security.application-groups": ["group.app.jurrian.travelplannerai"]
      },
      "associatedDomains": [
        "applinks:exai.expo.app",
        "webcredentials:exai.expo.app",
        "activitycontinuation:exai.expo.app"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@bacons/apple-colors",
      ["expo-router", {}],
      "expo-font",
      [
        "expo-quick-actions/icon/plugin",
        [
          "./assets/icons/scifi.png",
          "./assets/icons/awe.png",
          "./assets/icons/water.png",
          "./assets/icons/coffee.png",
          "./assets/icons/sunrise.png"
        ]
      ],
      "expo-quick-actions",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/bouquet.png",
          "resizeMode": "cover",
          "backgroundColor": "#000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactServerFunctions": true
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
